<table class="table table-striped">
    <thead>
        <tr>
            <th>{{ 'User Name' | trans }}</th>
            <th>{{ 'Transaction ID' | trans }}</th>
            <th>{{ 'Transaction Type' | trans }}</th>
            <th>{{ 'Email' | trans }}</th>
            <th>{{ 'Date' | trans }}</th>
            <th> {{ 'Actions' | trans }}</th>
        </tr>
    </thead>
    <tbody>
        {% for item in trans %}
            <tr>
                <td>{{ item.wsfusername }}</td>
                <td>{{ item.wsftxid }}</td>
                <td>{{ item.wsfsubtype }}</td>
                <td>{{ item.wsfemail }}</td>
                <td>{{ item.wsfpaydate.date }}</td>
                <td class="actions">
                    <a href="{{ path('paustianwebsitefeemodule_admin_deletetrans') }}/{{ item.id }}" class="fas fa-trash" onclick="return confirm('Are you sure you want to delete this subscription information?')"></a>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
     <form action="{{ path('paustianwebsitefeemodule_admin_deletetrans') }}" method='POST'>
        <button type='submit' name='deleteAll' value='deleteAll'>{{ 'Delete All' | trans }}</button>
    </form>

